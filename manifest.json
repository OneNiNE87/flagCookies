{
  "applications": {
    "gecko": {
      "strict_min_version": "58.0a1"
    }
  },
  "developer": {
    "name": "JanRie",
    "url": "https://github.com/jrie"
  },
  "default_locale": "en",
  "browser_action": {
    "browser_style": true,
    "default_title": "FlagCookies",
    "default_popup": "cookies.html",
    "default_icon": {
      "19": "icons/cookie_19.png",
      "38": "icons/cookie_38.png",
      "48": "icons/cookie_48.png",
      "64": "icons/cookie_64.png",
      "96": "icons/cookie_96.png",
      "128": "icons/cookie_128.png"
    }
  },
  "commands": {
    "toggle-profile": {
      "suggested_key": {
        "default": "Alt+Shift+A"
      },
      "description": "__MSG_ProfileShortcut__"
    }
  },
  "description": "__MSG_AddonDescription__",
  "icons": {
    "19": "icons/cookie_19.png",
    "38": "icons/cookie_38.png",
    "48": "icons/cookie_48.png",
    "64": "icons/cookie_64.png",
    "96": "icons/cookie_96.png",
    "128": "icons/cookie_128.png"
  },
  "homepage_url": "https://github.com/jrie/flagCookies",
  "manifest_version": 2,
  "name": "Flag Cookies",
  "version": "3.0.30",
  "permissions": ["cookies", "tabs", "webRequest", "<all_urls>", "webRequestBlocking", "storage", "unlimitedStorage", "notifications", "contextualIdentities"],
  "background": {
    "scripts": [
      "cookies.js"
    ]
  }
}
